<%- include layouts/header.ejs %>
<title>AlkoSzop - Wypełnij Dane</title>
<div class="page-data">

    <form method="post" action="/buy_adresy">
        <table cellpadding="11" align="center">
            <tr>
                <td class="label">ID Klienta</td>
                <td>:
                    <input type="text" name="ID_Klient" required>
                </td>
            </tr>
            <tr>
                <td class="label">Ulica</td>
                <td>:
                    <input type="text" name="ulica" required>
                </td>
            </tr>
            <tr>
                <td class="label">Numer Domu</td>
                <td>:
                    <input type="text" name="nr_domu" required>
                </td>
            </tr>
            <tr>
                <td class="label">Miejscowość</td>
                <td>:
                    <input type="text" name="miejscowosc" required>
                </td>
            </tr>
            <tr>
                <td class="label">Kod Pocztowy</td>
                <td>:
                    <input type="text" name="kod_pocztowy" required>
                </td>
            </tr>
            <tr>
                <td class="label"></td>
                <td>
                    &nbsp;
                    <input type="submit" name="submit" value="Kup">
                    <input type="button" name="cancel" value="Anuluj" class="cancel" onClick="cancelBuy()">
                </td>
            </tr>
        </table>
    </form>

</div>

<%- include layouts/footer.ejs %>
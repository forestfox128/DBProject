<%- include layouts/header.ejs %>
<title>AlkoSzop - Panel Administracyjny</title>
<div class="page-data">

    <form method="post" action="/adresy/edit/<%= data[0].ID_Klient %>">
        <table cellpadding="11">
            <tr>
                <td class="label">ID Klienta</td>
                <td>:
                    <input type="text" name="ID_Klient" value="<%= data[0].ID_Klient %>" required readonly
                           class="input-disabled">
                </td>
            </tr>
            <tr>
                <td class="label">Ulica</td>
                <td>:
                    <input type="text" name="ulica" value="<%= data[0].ulica %>" required>
                </td>
            </tr>
            <tr>
                <td class="label">Numer Domu</td>
                <td>:
                    <input type="text" name="nr_domu" value="<%= data[0].nr_domu %>" required>
                </td>
            </tr>
            <tr>
                <td class="label">Miejscowość</td>
                <td>:
                    <input type="text" name="miejscowosc" value="<%= data[0].miejscowosc %>" required>
                </td>
            </tr>
            <tr>
                <td class="label">Kod Pocztowy</td>
                <td>:
                    <input type="text" name="kod_pocztowy" value="<%= data[0].kod_pocztowy %>" required>
                </td>
            </tr>
            <tr>
                <td class="label"></td>
                <td>
                    &nbsp;
                    <input type="submit" name="submit" value="Zapisz">
                    <input type="button" name="cancel" value="Anuluj" class="cancel" onClick="cancelEditAdress()">
                </td>
            </tr>
        </table>
    </form>

</div>

<%- include layouts/footer.ejs %>